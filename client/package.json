    "fbemitter": "^2.1.1",
    "graceful-fs": "^4.2.3",
    "jquery": "^3.5.1",
    "js-yaml": "^3.13.1",
    "jss": "^9.8.7",
    "lodash": "^4.17.19",
    "minimatch": "^3.0.4",
    "moment": "^2.15.1",
    "mousetrap": "^1.6.5",
    "node-gyp": "^7.1.2",
    "optipng": "^2.1.0",
