{
  "name": "cs-ui",
  "version": "0.1.0",
  "description": "CSOS UI",
  "author": "Critical Stack Inc.",
  "license": "UNLICENSED",
  "private": true,
  "repository": "https://github.com/criticalstack/ui",
  "readme": "https://github.com/criticalstack/ui/blob/main/README.md",
  "resolutions": {
    "acorn": "^7.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "browserslist": [
    ">0.25%",
    "ie 11",
    "not op_mini all"
  ],
  "scripts": {
    "build:dev": "npx parcel build --no-minify --out-dir build --public-url /assets/ critical-stack.jsx critical-stack.css index.html",
    "build:prod": "npx parcel build -d build --no-source-maps --public-url /assets/ critical-stack.jsx critical-stack.css index.html",
    "lint": "eslint --ext .js --ext .jsx --cache ."
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticOutDir": "images",
        "staticPath": "images"
      },
      {
        "staticOutDir": "video",
        "staticPath": "video"
      }
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "@rjsf/core": "^2.0.0",
    "acorn": "^7.3.1",
    "browserify": "^16.5.1",
    "chart.js": "^2.9.4",
    "colors": "^1.4.0",
    "cron-parser": "^2.4.0",
    "cssnano": "^4.1.10",
    "d3": "^6.3.1",
